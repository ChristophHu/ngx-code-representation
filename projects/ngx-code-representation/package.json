  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophhu/ngx-code-representation.git"
  },
  "bugs": {
    "url": "https://github.com/christophhu/ngx-code-representation/issues"
  },
  "homepage": "https://github.com/christophhu/ngx-code-representation#readme",
  "peerDependencies": {
    "@angular/common": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@christophhu/ngx-icons": "^20.3.5",
    "ngx-highlightjs": "^14.0.1"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": [
      "src/lib/styles"
