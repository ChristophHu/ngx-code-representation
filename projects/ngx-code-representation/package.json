{
  "name": "@christophhu/ngx-code-representation",
  "version": "20.3.1",
  "description": "Library for Angular to present code.",
  "license": "MIT",
  "author": {
    "name": "Christoph Huschenhoefer",
    "email": "christoph@nxt.codes"
  },
  "keywords": [
    "angular",
